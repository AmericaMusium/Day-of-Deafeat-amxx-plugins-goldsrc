#include <amxmodx>
#include <fakemeta>

#define m_iNumTKs 478

public plugin_init()
{
	register_plugin("DOD TK Antikick","0.0","America")
    //WhatsApp +79101483016
    //For amxx 1.8.2
}   

public client_death(killer,victim,wpnindex,hitplace,TK)
{
    if(TK)
    {
        set_pdata_int(killer,m_iNumTKs, 0, 5) // RESET TEAMKILL
    }
    else
    {
           return;
    }
}
